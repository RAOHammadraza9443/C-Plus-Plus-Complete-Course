
#include<iostream>
using namespace std;
class sample
{
    static int count;
    public:
    void show();
    void showcount();
};
int sample::count;
void sample ::show(){
    cout<<"hello"<<endl;
    count++;
}
void sample::showcount(){
    cout<<"count ="<<count;
}
int main(){
   sample a,b,c;
   a.show();
   b.show();
   c.show();
   a.showcount(); 
    return 0;
}
