#include <iostream>
using namespace std;
int main() {
  int len,bth,area;
  cout<<"Enter length of rectangle :";
  cin>>len;
cout<<"Enter bteadth of rectangle :";
  cin>>bth;
  area = len*bth;
  cout<<"area of rectangle:"<<area;
  

   return 0;
}
