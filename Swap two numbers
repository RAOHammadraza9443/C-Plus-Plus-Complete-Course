// Swap two Numbers:


#include<iostream>
using namespace std;
template <class t>
void swapp(t &a,t &b){
    t temp = a;
    a = b;
    b = temp;
}
template<class t1,class t2>
float funcAverage(t1 a,t2 b){
    float avg = (a+b)/2.0;
    return avg;
}
int main(){
    float a;
    a = funcAverage(4,2);
    printf("The average of these number is %.3f\n",a);
    int x = 5, y = 7;
    swap(x,y);
    cout<<x<<endl<<y;
    return 0;
}
